component A(a: 32, b: 32) -> (c: 32) {
  cells {
    x = prim register(32, 32, 32, 32);
    y = my_mult;
  }

  wires {
    y.in = switch {
      x.in => x.out;
    }
    group B  {
      B[done] = 0 ;
      x.out = switch {
        1 == 2 => a;
        abc.in == 4 & abc.out => 4;
        abc.in == 4 & abc.out => 4;
      }
    }
    y.in = x.out;
  }

  control {
    seq {
      A;
      B;
      seq { A; B; }
      par { A; B; }
      if x.out with G {
         A;
      } else if x.out with G {
         B;
      } else {
         C;
      }
    }
  }
}
