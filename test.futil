import "primitives/std.lib";

component main(a: 32) -> (b: 32) {
  cells {
    r = prim std_reg(32);
    t = prim std_reg(32);
    add = prim std_add(32);
  }

  wires {
    add.left = a;
    add.right = r.out;
    r.in = add.out;
    t.in = add.out;
    b = add.out;

    b = a;
  }

  control {}
}
