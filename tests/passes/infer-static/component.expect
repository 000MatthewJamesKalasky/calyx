import "primitives/std.lib";
component main<"static"=3>(go: 1, clk: 1) -> (done: 1) {
  cells {
    a = prim std_reg(2);
    b = prim std_reg(2);
    c = prim std_reg(2);
  }
  wires {
    group A<"static"=1> {
      a.in = 2'd0;
      a.write_en = 1'd1;
      A[done] = a.done;
    }
    group B<"static"=1> {
      b.in = 2'd1;
      b.write_en = 1'd1;
      B[done] = b.done;
    }
    group C<"static"=1> {
      c.in = 2'd2;
      c.write_en = 1'd1;
      C[done] = c.done;
    }
  }

  control {
    @static(3) seq {
      @static(1) A;
      @static(1) B;
      @static(1) C;
    }
  }
}
