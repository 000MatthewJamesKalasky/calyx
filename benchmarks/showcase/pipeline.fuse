// BEGIN macro definitions

define(N, 10)
define(DATATYPE, float)

// END macro definitions

import "printer.cpp" {
  def extern print_float_vec(c: float[N]);
}

decl A: DATATYPE[N];
decl B: DATATYPE[N];

// we want this to be pipelined
for (let i = 0..N) {
  let a = A[i];
  ---
  let b = a + 10.0;
  let c = b * a;
  B[i] := c / 2.0;
}
---
print_float_vec(B);
